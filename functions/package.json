    "mongodb": "^6.0.0",
    "openai": "^4.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "eslint-config-google": "^0.14.0"
  },
  "private": true
